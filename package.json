{
  "name": "polestar-icons",
  "author": "NKIA",
  "version": "0.9.17",
  "description": "Polestar UI Icons",
  "main": "./lib/index.js",
  "keywords": [
    "React",
    "Icons",
    "Polestar"
  ],
  "repository": "nkiateam/polestar-icons",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nkiateam/polestar-icons/issues"
  },
  "homepage": "https://github.com/nkiateam/polestar-icons#readme",
  "scripts": {
    "docs": "styleguidist server",
    "docs:build": "rimraf docs && styleguidist build && git add docs",
    "examples:build": "rimraf examples/build && cross-env NODE_ENV=production webpack",
    "prepublish": "rimraf lib && babel src --out-dir lib --copy-files",
    "generate": "node_modules/.bin/cross-env NODE_ENV=production babel-node --presets=env --plugins=transform-async-to-generator ./generator/app.js",
    "gen": "node_modules/.bin/cross-env NODE_ENV=production babel-node --presets=env --plugins=transform-async-to-generator ./generator/app.js",
    "lint": "eslint . --cache && echo \"eslint: no lint errors\"",
    "start": "webpack-dev-server --open"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "peerDependencies": {
    "react": "^16.1.0",
    "react-dom": "^16.1.0"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cli-clear": "^1.0.4",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "figlet": "^1.2.0",
    "file-loader": "^1.1.5",
    "inquirer": "^5.0.1",
    "jsdom": "^11.6.0",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.8",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-styleguidist": "^6.0.33",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "svg2ttf": "^4.1.0",
    "ttf2eot": "^2.0.0",
    "ttf2woff": "^2.0.1",
    "ttf2woff2": "^2.0.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
