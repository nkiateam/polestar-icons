{
  "name": "polestar-icons",
  "author": "NKIA",
  "version": "0.9.61",
  "description": "Polestar UI Icons",
  "main": "./lib/index.js",
  "keywords": [
    "React",
    "Icons",
    "Polestar"
  ],
  "repository": "nkiateam/polestar-icons",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nkiateam/polestar-icons/issues"
  },
  "homepage": "https://github.com/nkiateam/polestar-icons#readme",
  "scripts": {
    "svgo": "svgo ./src/svg/server.svg -o ./src/svg/server.min.svg",
    "svgr": "rimraf src/svgComponents/* && npx @svgr/cli -d src/svgComponents src/svgs",
    "docs": "styleguidist server",
    "build:docs": "rimraf docs && styleguidist build && git add docs",
    "build:examples": "rimraf examples/build && cross-env NODE_ENV=production webpack",
    "prepublishOnly": "rimraf lib && babel src --out-dir lib --copy-files",
    "generate": "node_modules/.bin/cross-env NODE_ENV=production babel-node --presets=@babel/env --plugins=@babel/transform-async-to-generator ./generator/app.js",
    "gen": "node_modules/.bin/cross-env NODE_ENV=production babel-node --presets=@babel/env --plugins=@babel/transform-async-to-generator ./generator/app.js",
    "lint": "eslint . --cache && echo \"eslint: no lint errors\"",
    "http-server": "http-server examples/ -p 4101 -o",
    "start": "webpack-dev-server --open"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@loadable/component": "^5.10.3",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-async-to-generator": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@svgr/cli": "^4.3.3",
    "autoprefixer": "^9.7.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-minify": "^0.5.1",
    "cli-clear": "^1.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.6.0",
    "eslint-config-polestar": "^0.1.2",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-flowtype": "^4.4.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "figlet": "^1.2.4",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "inquirer": "^5.2.0",
    "jsdom": "^11.12.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
