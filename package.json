{
  "name": "polestar-icons",
  "author": "NKIA",
  "version": "0.10.22",
  "description": "Polestar UI Icons",
  "main": "./lib/index.js",
  "keywords": [
    "React",
    "Icons",
    "Polestar"
  ],
  "repository": "nkiateam/polestar-icons",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nkiateam/polestar-icons/issues"
  },
  "homepage": "https://github.com/nkiateam/polestar-icons#readme",
  "scripts": {
    "svgo": "svgo ./src/svg/server.svg -o ./src/svg/server.min.svg",
    "svgr": "rimraf src/svgComponents/* && npx @svgr/cli -d src/svgComponents src/svgs",
    "docs": "styleguidist server",
    "build:docs": "rimraf docs && styleguidist build && git add docs",
    "build:examples": "rimraf examples/build && cross-env NODE_ENV=production webpack",
    "prepublishOnly:test": "rimraf lib && babel src --out-dir lib --copy-files",
    "gen:svg": "npm run svgr && node scripts/createSvgIcons.js",
    "generate": "node_modules/.bin/cross-env NODE_ENV=production babel-node --presets=@babel/env --plugins=@babel/transform-async-to-generator ./generator/app.js",
    "gen:font": "node_modules/.bin/cross-env NODE_ENV=production babel-node --presets=@babel/env --plugins=@babel/transform-async-to-generator ./generator/app.js",
    "gen": "npm run gen:font && npm run gen:svg",
    "lint": "eslint . --cache && echo \"eslint: no lint errors\"",
    "http-server": "http-server examples/ -p 4101 -o",
    "start": "webpack-dev-server --open"
  },
  "directories": {
    "doc": "docs",
    "example": "examples",
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@loadable/component": "^5.13.1",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@svgr/cli": "^4.3.3",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-minify": "^0.5.1",
    "cli-clear": "^1.0.4",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^7.8.1",
    "eslint-config-polestar": "^1.0.3",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^2.5.1",
    "figlet": "^1.2.4",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^0.11.1",
    "inquirer": "^5.2.0",
    "jsdom": "^11.12.0",
    "mini-css-extract-plugin": "^0.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "react": "^16.13.1",
    "react-dev-utils": "^9.1.0",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-styleguidist": "^10.2.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "svg2ttf": "^4.3.0",
    "svgo": "^1.3.2",
    "terser-webpack-plugin": "^2.3.8",
    "ttf2eot": "^2.0.0",
    "ttf2woff": "^2.0.1",
    "ttf2woff2": "^2.0.3",
    "url-loader": "^2.3.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "types": "./lib/polestar-icons.d.ts"
}
